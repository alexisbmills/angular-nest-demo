{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "sourceMap": true,
    "outDir": "../dist/server-app",
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitReturns": true
  },
  "exclude": ["node_modules", "dist"]
}
